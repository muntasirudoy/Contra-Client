{
    "rewrites":[{"source":"/(.*)", "destination":"/index.html"}],
    "version": 2,
    "builds": [
      {
        "src": "your-source-directory",
        "use": "@vercel/node",
        "config": { "maxLambdaSize": "2048mb" },
        "env": { "CUSTOM_ENV_VAR": "value" },
        "buildCommand": "npm install --force && npm run build"
      }
    ]
}
    
  
  